<% @articles.each do |a| %>
</br></br>
<%= a.title %>
</br>
<%= "Created: " +  a.created_at.to_s(:long) %>
<%= button_to "Publish Article", articles_show_path,id: a.title, params: {:id => a.id, :title => a.title, :body => a.body} %>
<%= button_to "Delete Article", articles_delete_path, id:"delete_" + a.title, params: {:id => a.id} %>
<%= link_to "View Article", articles_display_path(:id => a.id), id:"view_" + a.title %>
<% end %>
