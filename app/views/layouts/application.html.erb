<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'shared/no_blog_error' %>
  <%= render 'shared/not_signed_in_error'%>
  <% if admin_signed_in? %>
    <%= link_to "Create New Article", articles_create_path %>
  <% end %>
<%= yield %>
</body>
</html>
